%form{ method: 'post', enctype: 'multipart/form-data'}
  .label Title
  %input{type: 'text', name: 'title'}
  .label Description
  %input{type: 'text', name: 'descr'}
  .label Edit tags
  %input{type: 'text', name: 'tags'}
  .label Files
  %input{type: 'file', name: 'files', multiple: 'multiple'}
  %input{type: 'submit', value: 'submit'}